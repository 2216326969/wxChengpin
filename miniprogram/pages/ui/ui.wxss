.img{
  width: 100%;
  height: 180px;
}
.txt{
  margin-top: 6%;
}
.title{
  font-size: 18px;
  font-weight: bold;
  text-align: center;
  margin: 3% 0;
  color: #333;
}
.txt .viw{
  display: inline-block;
  font-weight: bold;
  color: #666;
}
.text{
  width: 88%; 
  margin: auto;
  font-size: 27rpx;
  text-align: center;
  color: #999;
}
.txt .txt_ig{
  text-align: center;
  display: block;
  margin: 4% auto;
  box-shadow: 2px 2px 10px 5px rgba(0,0,0,0.2);
  width: 90%;
}
.box{
  display: flex;
  justify-content: space-around;
  padding-top: 2%;
}
.box image{
  width: 60rpx;
  height: 60rpx;
  box-shadow:none;
  padding-top: 27%;
}
.sing_bg{
  background-color: rgb(73,  218 , 170);
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  box-sizing: border-box;
  margin: auto;
}
.box .sing{
  width: 28%;
  padding:3% 0;
  box-shadow: 2px 2px 10px 5px rgba(0,0,0,0.2);
  border-radius: 3px;
  text-align: center;
}
.img_a{
  width: 100rpx !important;
  height: 100rpx !important;
}
.box .img_a{
  padding-top: 10% !important;
}
.box_txt{
  color: #999;
  font-size: 26rpx;
}
/*90 90 252*/
.box_txta{
  color: #999;
  font-size: 22rpx;
}
.box_btn{
  background: rgb(90,90,252);
  padding:5% 4%;
  width: 80%;
  margin:8% auto 4% auto;
  border-radius: 23px;
  color: #fff;
  font-size: 22rpx;
}
.skill{
  background: #eee;
  padding: 5%;
  margin: auto;
  text-align: center;
  color: #999;
  font-size: 28rpx;
  
}
.skill_a image{
  width: 90%;
  height: 230rpx;
}
.skill_a{
  margin-top: 3%;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.skill_a>view{
    width: 50%;
    margin-top: 2%;
}

.content1{
  /* border: 1px solid #333; */
  width: 85%;
  margin: auto;
  margin-top: 4%;
  box-shadow: 2px 2px 10px 5px rgba(0,0,0,0.2);
  border-radius: 5px;
}
.active{
  background-color: #0aa1ed;
  color: #fff;
  border: 0;
}
.btns view{
  border: 1px solid #0aa1ed;
  padding: 2% 4%;
  width: 20%;
  text-align: center;
  margin-top: 2%;
  border-radius: 8px;
  font-size: 22rpx;
}
.btns{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 85%;
  margin: auto;
}

.content1 image{
  width: 60%;
  height: 215rpx;
  padding: 5% 2%;
  margin: auto;
  display: block;
}
.one_a view{
  font-size: 25rpx;
  color: #999;
  padding: 2% 3%;
}
.one_title{
  font-size: 32rpx !important;
}
.title_d{
  width: 80%;
  margin: auto;
  font-size: 24rpx;
  color: #999;
  text-align: center;
}
.more{
  /*
  2 187 189
  1 137 199
  **/
  background-image: linear-gradient(to right, rgb(2, 187, 189), rgb(1, 137, 199));
  margin-top: 8%;
  padding: 5% 0;
}
.more_tit{
  text-align: center;
  color: #fff;
  font-size: 18px;
  font-weight: bold;
}
.btn{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.btn view{
  width: 30%;
  border: 1px solid #fff;
  padding: 2% 3%;
  text-align: center;
  border-radius: 23px;
  margin-top: 5%;
  color: #fff;
}
.btn_active{
  background: #fff;
  color: rgb(1,162,194) !important;
}