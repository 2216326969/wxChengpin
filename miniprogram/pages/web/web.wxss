.img{
  width: 100%;
  height: 159px;
}
.bdy{
  background: #fff;
}
.web_a image{
  width: 50rpx;
  height: 40rpx;
}
.web_a{
  display: flex;
  justify-content: space-around;
  margin-top: 8%;
  height: 15vh;
}
.web_txt{
  text-align: center;
  font-weight: bold;
  font-size: 18px;
  padding-bottom: 2%;
  position: relative;
  top: -17%;
}
.ig image{
  border: 0;
  width: 90%;
  margin: auto;
  display: block;
  height: 27vh;
}
.content1{
  width: 85%;
  margin: auto;
  margin-top: 4%;
  box-shadow: 2px 2px 10px 5px rgba(0,0,0,0.2);
  border-radius: 5px; 
  padding: 4% 2%;
}
.btns{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 85%;
  margin: auto;
}
.btns view{
  border: 1px solid #0aa1ed;
  padding: 2% 4%;
  width: 20%;
  text-align: center;
  margin-top: 2%;
  border-radius: 8px;
  font-size: 24rpx;
}
.content1 image{
  width: 100%;
  height: 260rpx;
}
.content1 .tit{
  font-weight: bold;
  padding: 2%;
}
.content1 .text{
  font-size: 24rpx;
  color: #666;
}
.active{
  background-color: #275df8;
  color: #fff;
}
.tisheng{
  padding-top: 9%;
}
.tisheng_btns{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 85%;
  margin: auto;
}
.tisheng_btns view{
  border: 1px solid #eee;
  padding:  4%;
  width: 40%;
  text-align: center;
  margin-top: 2%;
  border-radius: 8px;
  font-size: 24rpx;
  background-color: #eee;
}
.actives{
  color: #fff;
  background: linear-gradient(to right, #ff9f2b 0%,#ff6c22 100%);
}
.titles{
  font-size: 18px;
  font-weight: bold;
  text-align: center;
  padding-bottom: 3%;
}
.tex view{
  font-size: 28rpx;
  color: #333;
}
.tex{
  padding: 1% 3% ;
}
.more{
  /*
  2 187 189
  1 137 199
  **/
  background-image: linear-gradient(to right, rgb(2, 187, 189), rgb(1, 137, 199));
  margin-top: 8%;
  padding: 5% 0;
}
.more_tit{
  text-align: center;
  color: #fff;
  font-size: 18px;
  font-weight: bold;
}
.btn{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.btn view{
  width: 30%;
  border: 1px solid #fff;
  padding: 2% 3%;
  text-align: center;
  border-radius: 23px;
  margin-top: 5%;
  color: #fff;
}
.btn_active{
  background: #fff;
  color: rgb(1,162,194) !important;
}