			<image src="../../images/php1.png"></image>
			<image src="../../images/c1.png"></image>
			<image src="../../images/linux1.png"></image>
		</view>
	</view>
	<!-- 学校环境 -->
	<view class="school">
		<view class="school_title">
			<view class="school_titlea">
				<view class="viw">学校环境</view>		环境
			</view>
			<view class="school_titleb">企业职场氛围 全程项目实训</view>
			<view class="school_titlec">项目研发中心 一体机项目教学</view>
		</view>
		<view class="picture">
			<image src="../../images/1.jpg"></image>
			<image src="../../images/2.jpg"></image>
			<image src="../../images/3.jpg"></image>
			<image src="../../images/5.jpg"></image>
